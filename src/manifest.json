{
  "manifest_version": 2,
  "name": "MilliCache",
  "version": "1.0.13",
  "description": "Inspect MilliCache headers in DevTools.",
  "devtools_page": "devtools/devtools.html",
  "permissions": [
    "devtools",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "millicache@headers.dev",
      "strict_min_version": "96.0",
      "update_url": "https://millipress.github.io/millicache-firefox-ext/updates.json"
    }
  }
}
